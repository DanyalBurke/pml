      "source": [
        "def square(x):\n",
        "    return x * x\n",
        "\n",
        "def times2(x):\n",
        "    return x * 2\n",
        "\n",
        "def average(x1, x2, x3, x4, x5):\n",
        "    return (x1 + x2 + x3 + x4 + x5) / 5.0\n",
        "\n",
        "print(square(5))\n",
        "print(times2(5))\n",
        "print(average(1, 2, 3, 4, 5))"
      ],
